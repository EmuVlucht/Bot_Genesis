# ============================================
# VAULTVERSE FRONTEND ENVIRONMENT (.env)
# File ini harus ditempatkan di: client/.env
# ============================================

# ============================================
# API CONFIGURATION
# ============================================
VITE_API_URL=http://localhost:5000/api

# Production API URL (uncomment untuk production):
# VITE_API_URL=https://api.vaultverse.com/api

# ============================================
# APP CONFIGURATION
# ============================================
VITE_APP_NAME=VaultVerse
VITE_APP_VERSION=1.0.0

# ============================================
# FEATURE FLAGS
# ============================================
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_EXPORT_IMPORT=true

# ============================================
# ============================================

